{
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "dynamic.js"
      }
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
